@{
    Layout = "~/Views/Layout/LayoutBase.cshtml";
}
<!DOCTYPE html>

<html>
<body>
    @*TopImage*@
    <div id="informationheader">
        <img src="~/Content/Resources/logowithtext.png" id="logo" />
    </div>

    @*NavBar*@
    <nav class="navbar navbar-icon-top navbar-expand-lg navbar-dark" id="navigationBar">
        <img src="~/Content/Resources/text2.png" id="logotext" />
        <div class="collapse navbar-collapse" id="navigation">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="#" id="menuitem">Overview</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#" id="menuitem">Contacts</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#" id="menuitem">My Assignments</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#" id="menuitem" onclick="SignUp()" data-toggle="modal" data-target="#exampleModalCenter">Sign Up</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#" id="menuitem" onclick="SignIn()" data-toggle="modal" data-target="#exampleModalCenter">Sign In</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="Main/Account" id="menuitem">Account</a>
                </li>
            </ul>
        </div>
    </nav>




    @*Container*@
    <div class="container"> @RenderBody() </div>

    @*<div class="footer"></div>*@


</body>
</html>
